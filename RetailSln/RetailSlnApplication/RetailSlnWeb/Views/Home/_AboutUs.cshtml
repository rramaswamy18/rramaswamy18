@model ArchitectureLibraryModels.AboutUsModel
@using ArchitectureLibraryCacheData
@{
    var clientId = ArchLibCache.ClientId;
    string divWidth;
    bool mobileDevice = Request.Browser.IsMobileDevice;
    try
    {
        var userAgent = HttpContext.Current.Request.UserAgent.ToLower();
        //Not using right now
        if (userAgent.Contains("iphone;")) //iPhone
        {
            divWidth = "90%;";
        }
        else if (userAgent.Contains("ipad;") || userAgent.Contains("macintosh;"))
        {
            divWidth = "63%";
        }
        else
        {
            divWidth = "45%;";
        }
    }
    catch
    {
        divWidth = "45%;";
    }
    //Using this right now
    if (mobileDevice)
    {
        divWidth = "90%;";
    }
    else
    {
        divWidth = "54%;";
    }
    //mobileDevice = true;
}
