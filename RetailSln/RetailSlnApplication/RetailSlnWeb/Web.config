	<log4net>
		<root>
			<level value="ALL"></level>
			<appender-ref ref="AdoNetAppender"></appender-ref>
		</root>
		<appender name="AdoNetAppender" type="log4net.Appender.AdoNetAppender">
			<bufferSize value="1" />
			<connectionType value="System.Data.SqlClient.SqlConnection, System.Data, Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
			<connectionStringName value="DatabaseConnectionStringLog" />
			<commandText value="INSERT ArchLib.ExceptionLog(LogDateTime, ApplicationName, IpAddress, ExecUniqueId, LoggedInUserId, CallingAssemblyName, ExecutingAssemblyName, CurrentMethodDeclaringType, MethodName, CallerLineNumber, ThreadNum, LogType, Logger, Message, DebugData, Exception) VALUES(@LogDateTime, @ApplicationName, @IPAddress, @ExecUniqueId, @LoggedInUserId, @CallingAssemblyName, @ExecutingAssemblyName, @CurrentMethodDeclaringType, @MethodName, @CallerLineNumber, @ThreadNum, @LogType, @Logger, @Message, @DebugData, @Exception)" />
			<parameter>
				<parameterName value="@LogDateTime" />
				<dbType value="DateTime" />
				<layout type="log4net.Layout.RawTimeStampLayout" />
			</parameter>
			<parameter>
				<parameterName value="@ThreadNum" />
				<dbType value="String" />
				<size value="255" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%thread" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@LogType" />
				<dbType value="String" />
				<size value="50" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%level" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@Logger" />
				<dbType value="String" />
				<size value="500" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%logger" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@Message" />
				<dbType value="String" />
				<size value="2000" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%message" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@DebugData" />
				<dbType value="String" />
				<size value="1073741822" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{DebugData}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@Exception" />
				<dbType value="String" />
				<size value="1073741822" />
				<layout type="log4net.Layout.ExceptionLayout" />
			</parameter>
			<parameter>
				<parameterName value="@ApplicationName" />
				<dbType value="String" />
				<size value="50" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{ApplicationName}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@IPAddress" />
				<dbType value="String" />
				<size value="100" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{IPAddress}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@ExecUniqueId" />
				<dbType value="String" />
				<size value="100" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{ExecUniqueId}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@LoggedInUserId" />
				<dbType value="String" />
				<size value="100" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{LoggedInUserId}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@CallingAssemblyName" />
				<dbType value="String" />
				<size value="2000" />
				<layout type="log4net.Layout.PatternLayout">
