@using ArchitectureLibraryCacheData
<br style="clear: both;" />
<div class="container" style="background-color: #f3f3f3; width: 100%;">
    <table align="center" class="table table-responsive">
        <tr>
            <td style="text-align: left; width: 25%;">
                <i class="fa fa-facebook-square" style="color: #4267b2; font-size: 36px"></i>
                <i class="fa fa-twitter-square" style="color: #00acee; font-size: 36px"></i>
                @*<i class="fa fa-youtube-square" style="color: #cd201f; font-size: 36px"></i>
                <i class="fa fa-linkedin-square" style="color: #0e76a8; font-size: 36px"></i>*@
                <br style="clear: both;" />
                <span id="loggedInUserFullName" style="color: #000000;"></span>
                <br style="clear: both;" />
                <span id="loggedInUserEmailAddress" style="color: #000000;"></span>
            </td>
            <td style="text-align: center; width: 50%;">
                <span style="font-family: sans-serif; font-size: 13px; color: #7a7a7a;">Powered by</span>&nbsp;<span style="color: #0170b9; font-size: 13px; font-weight: bold;">Gajaashwa Inc.</span>&nbsp;
                <span style="font-family: sans-serif; font-size: 12px; color: #000000;">&copy; Copyright @DateTime.Now.Year</span>
                <br />
                @Html.Partial("_FooterMenu")
            </td>
            <td style="text-align: right; width: 25%;">
                @ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "AddressLine1", "")
                <br />
                @ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "AddressCityName", "")&nbsp;@ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "AddressStateAbbrev", "")&nbsp;@ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "AddressZipCode", "")&nbsp;@ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "AddressZipPlus4", "")
                <br />
                @ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "AddressCountryName", "")
                <br />
                <i style="color: #000000; font-weight: bold;" class="fa fa-phone"></i>
                <a href="tel: @ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "ContactTextPhoneHref", "")">@ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "ContactTextPhoneFormatted", "")</a>
                <br />
                <img src="~/Images/SMSIcon1.png" style="height: 27px; width: 27px;" />
                <a href="sms: @ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "ContactTextPhoneHref", "")">@ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "ContactTextPhoneFormatted", "")</a>
                <br />
                <img src="~/Images/WhatsApp1.png" style="height: 27px; width: 27px;" />
                <a target="_blank" href="https://api.whatsapp.com/send?phone=@(ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "ContactWhatsAppPhone", ""))">@ArchLibCache.GetApplicationDefault(ArchLibCache.ClientId, "ContactWhatsAppPhoneFormatted", "")</a>
            </td>
        </tr>
    </table>
</div>
