@using ArchitectureLibraryCacheData
@using ArchitectureLibraryModels
@using RetailSlnCacheData
@{
    //DivineBija - 97, 98, 99
    var clientId = ArchLibCache.ClientId;
    var clientName = ArchLibCache.ClientName;
    var imageTitle = ArchLibCache.GetApplicationDefault(clientId, "BusinessName1", "") + " | " + ArchLibCache.GetApplicationDefault(clientId, "BusinessType", "");
    int mainCategoryCount = 5;
    SessionObjectModel sessionObjectModel = (SessionObjectModel)Session["SessionObject"];
    string aspNetRoleName;
    bool isLoggedIn;
    if (sessionObjectModel == null)
    {
        aspNetRoleName = "DEFAULTROLE";
        isLoggedIn = false;
    }
    else
    {
        aspNetRoleName = sessionObjectModel.AspNetRoleName;
        isLoggedIn = true;
    }
    isLoggedIn = isLoggedIn && User.Identity.IsAuthenticated;
    var categoryModels = RetailSlnCache.AspNetRoleParentCategoryCategoryModels[aspNetRoleName][0];
    int i;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="author" content="Gajaashwa Inc." />
    <meta name="title" content="@ArchLibCache.GetApplicationDefault(clientId, "Head", "Title")" />
    <meta name="description" content="@ArchLibCache.GetApplicationDefault(clientId, "Head", "Description")" />
    <meta name="keywords" content="@ArchLibCache.GetApplicationDefault(clientId, "Head", "Keywords")" />
    <meta name="robots" content="index, follow" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="language" content="English" />
    <meta name="revisit-after" content="1 days" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ArchLibCache.GetApplicationDefault(clientId, "Head", "Title")</title>
    <link rel="shortcut icon" href="~/ClientSpecific/@(clientId)_@(clientName)/Documents/Images/Favicon.ico" />
    @Styles.Render("~/Content/css0")
    @Scripts.Render("~/bundles/scripts0")

    <link rel="stylesheet" href="~/Include/Site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Patua+One|Open+Sans|Lato|Roboto" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" />

    <link href="~/Include/font-awesome/css/all.css" rel="stylesheet" />
    <link href="~/Include/font-awesome/css/all.min.css" rel="stylesheet" />
    @RenderSection("scripts", required: false)
</head>
<body>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>*@
    <script type="text/javascript">
        $(document).ready(function () {
            window_onload();
            shoppingCartSummary_onclick();
        });
        function window_onload() {
            //$(".se-pre-con").fadeOut("slow");
            alert("Implement window on load in your page");
        }
    </script>
    <script src="~/Include/orderItemCode0.js" type="text/javascript"></script>
    <div id="divScrollIntoView"></div>
    <div class="body-container">
        <header class="header-section sticky">
            <div class=" top-bar">
                <div class="row">
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                        <div class="logo-section">
                            <a href="@Url.Action("Index", "Home")" class="navbar-right-link">
                                <img alt="@imageTitle" src="~/ClientSpecific/@(clientId)_@(clientName)/Documents/Images/Image_000.webp" width="117" height="117" title="@imageTitle" />
                            </a>
                            <p class="company-name font-bold" style="vertical-align: top;">
                                <span style="color: #9401d9; font-size: 22px; font-weight: bold;">@ArchLibCache.GetApplicationDefault(clientId, "BusinessNameWord1", "")</span><br /><span style="color: #ff0000; font-size: 14px; font-weight: bold;">@ArchLibCache.GetApplicationDefault(clientId, "BusinessNameWord2", "")</span><br /><span style="color: #000000; font-size: 14px; font-weight: bold;">@ArchLibCache.GetApplicationDefault(clientId, "BusinessNameWord3", "")</span>
                                <br style="clear: both;" />
                            </p>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                        <div class="search-container">
                            <input id="searchTerm" class="search-input" placeholder="Search" type="text">
                            <button class="search-button" onclick="javascript: return searchTermButton_onclick('searchTerm');">
                                <img alt="Search" src="https://static.vecteezy.com/system/resources/thumbnails/014/440/989/small/search-black-shadow-icon-socialicon-set-png.png" />
                            </button>
                        </div>
                        <div id="divErrorMessage" style="color: #ff0000; font-family: Arial; font-size: 12px; font-weight: bold;">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                        <div class="nav navbar-nav navbar-right">
                            <div class="nav-right-feature" style="padding-right: 0px; text-align: center;">
                                <a href="@Url.Action("Index", "Home")" class="navbar-right-link">
                                    <i class="fas fa-home"></i> <br />
                                    Home
                                </a>
                            </div>
                            <div class="nav-right-feature" style="min-width:100px; padding-right: 0px; text-align: center;">
                                <a href="javascript: void(0);" onclick="javascript: return shoppingCart_onclick();" style="color: #333333; font-family: Arial, Helvetica, sans-serif; font-size: 14px; font-weight: 400;" title="View Cart - @imageTitle">
                                    <i class="fas fa-shopping-cart" style="color: #000; font-size: 15px"></i>
                                    <span id="shoppingCartItemsCount" style="display: none;">0</span>
                                    <span class="badge badge-warning shoppingCartItemsCount" id="shoppingCartTotalAmount" style="background-color: #9a031e;"></span>
                                    <br />
                                    <p class="text-overflow-ellipsis">View Cart</p>
                                </a>
                            </div>
                            <div class="nav-right-feature" style="padding-right: 0px; text-align: center;">
                                <a href="javascript: void(0);" onclick="javascript: return checkoutValidate_onclick();" class="navbar-right-link" title="Checkout - @imageTitle">
                                    <img alt="Checkout - @imageTitle" src="~/Images/Checkout2.png" style="height: 20px; width: 20px;" title="Checkout - @imageTitle">
                                    <br />
                                    Checkout
                                </a>
                            </div>
                            <div class="nav-right-feature" style="padding-right: 0px; text-align: center;">
                                <a class="navbar-right-link" href="@Url.Action("RegisterUserProf", "Home")" title="Register - @imageTitle">
                                    <i class="fas fa-user-plus"></i>
                                    <br />
                                    Register
                                </a>
                            </div>
                            <div class="nav-right-feature" style="padding-right: 0px; text-align: center;">
                                <a class="navbar-right-link @(isLoggedIn ? "disabled-link" : "")" href="@Url.Action("LoginUserProf", "Home")" title="Login / Sign In - @imageTitle">
                                    <i class="fas fa-user"></i>
                                    <br />
                                    Sign In
                                </a>
                            </div>
                            <div class="nav-right-feature" style="padding-right: 0px; text-align: center;">
                                <a class="navbar-right-link @(isLoggedIn ? "" : "disabled-link")" href="@Url.Action("Logout", "Home")" title="Logout - @imageTitle">
                                    <i class="fas fa-sign-out-alt"></i>
                                    <br />
                                    Logout
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <nav class="navbar navbar-default">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <div class="nav navbar-nav navbar-right-hamburger">
                            <div class="nav-hamburger-feature" style=" padding-right: 0px; text-align: center;">
                                <a href="javascript: void(0);" onclick="javascript: return shoppingCart_onclick();" style="color: #fff; font-family: Arial, Helvetica, sans-serif; font-size: 14px; font-weight: 400;" title="View Cart - @imageTitle">
                                    <span style="white-space: nowrap;color: #fff">View cart</span>
                                    <span id="shoppingCartItemsCount1" style="display: none;">0</span><br />
                                    <span class="badge badge-warning shoppingCartItemsCount" id="shoppingCartTotalAmount1" style="background-color: white; color: #9a031e;">$0.00</span>
                                </a>
                            </div>
                            <div class="nav-hamburger-feature" style="padding-right: 0px; text-align: center;">
                                <a href="javascript: void();" onclick="javascript: return checkoutValidate_onclick();" class="navbar-right-link" title="Checkout - @imageTitle">
                                    Checkout
                                </a>
                            </div>
                            <div class="nav-hamburger-feature" style="padding-right: 0px; text-align: center;">
                                <a href="@Url.Action("RegisterUserProf", "Home")" class="navbar-right-link" title="Register - @imageTitle">
                                    Register
                                </a>
                            </div>
                            <div class="nav-hamburger-feature" style="padding-right: 0px; text-align: center;">
                                <a href="@Url.Action("LoginUserProf", "Home")" class="navbar-right-link" title="Sign In - @imageTitle">
                                    Login
                                </a>
                            </div>
                            <div class="nav-hamburger-feature" style="padding-right: 0px; text-align: center;">
                                <a href="@Url.Action("Logout", "Home")" class="navbar-right-link @(isLoggedIn ? "" : "disabled-link")" title="Logout - @imageTitle">
                                    Logout
                                </a>
                            </div>
                        </div>
                        <div class="hamburger">
                            <button type="button" class="navbar navbar-toggle collapsed custom-toggle" data-toggle="collapse" data-target="#feature-section-navbar" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <i class="fas fa-bars"></i>
                                <i class="fas fa-times toggle-icon"></i>
                            </button>
                        </div>
                    </div>
                    <div class="collapse navbar-collapse custom-navbar" id="feature-section-navbar">
                        <ul class="nav navbar-nav">
                            @for (i = 0; i < mainCategoryCount && i < categoryModels.Count; i++)
                            {
                                <li>
                                    <span id="menuCategoryId@(i)" style="display: none;">@categoryModels[i].CategoryId</span>
                                    <a class="nav-links" id="menuCategory@(i)" href="javascript: void(0);" onclick="javascript: return categoryId_onclick(@categoryModels[i].CategoryId, 1, @categoryModels.Count);" style="font-weight: bold;" data-toggle="collapse" data-target="#feature-section-navbar">@categoryModels[i].CategoryDesc</a>
                                </li>
                            }

                            @{ 
                                int j = i;
                            }
                            @for (; j < categoryModels.Count; j++)
                            {
                            <li class="more-items-xs visible-xs hidden">
                                <span id="menuCategoryId@(j)" style="display: none;">@categoryModels[j].CategoryId</span>
                                <a class="nav-links" id="menuCategory@(j)" href="javascript: void(0);" onclick="javascript: return categoryId_onclick(@categoryModels[j].CategoryId, 1, @categoryModels.Count);" style="font-weight: bold;" data-toggle="collapse" data-target="#feature-section-navbar">@categoryModels[j].CategoryDesc</a>
                            </li>
                            }
                            @if (categoryModels.Count > 1)
                            {
                                <li class="dropdown hidden-xs">
                                    <a aria-haspopup="true" aria-expanded="false" class="navigation-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" style="font-weight: bold;">
                                        More...&nbsp;<span class="caret" style="color: #ffffff; font-weight: bold;"></span>
                                    </a>
                                    <ul class="dropdown-menu" style="margin-top: -1px;">
                                        @for (; i < categoryModels.Count; i++)
                                        {
                                            <li>
                                                <span id="menuCategoryId@(i)" style="display: none;">@categoryModels[i].CategoryId</span>
                                                <a class="nav-links" id="menuCategory@(i)" href="javascript: void(0);" onclick="javascript: return categoryId_onclick(@categoryModels[i].CategoryId, 1, @categoryModels.Count);" style="font-weight: bold;" data-toggle="collapse" data-target="#feature-section-navbar">@categoryModels[i].CategoryDesc</a>
                                            </li>
                                        }
                                    </ul>
                                </li>

                                <li class="hidden-xs" id="current-active-feature-item">
                                    <a href="javascript: void(0);" style="cursor: default;">
                                        <span id="current-active-feature" style="color: #61ce70; font-size: 18px; font-weight: bold;"></span>
                                    </a>
                                </li>
                            }
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        <div class="center main-body" style="padding: 0px; text-align: center; width: 100%; overflow-x: hidden;">
            <main role="main" class="pb-3" style="overflow-x: hidden; ">
                @RenderBody()
            </main>
        </div>
        @Html.Partial("_Footer")
    </div>
    @if (categoryModels.Count > 1)
    {
        <script>
            document.addEventListener("DOMContentLoaded", function () {
                var navLinks = document.querySelectorAll('.nav-link');
                navLinks.forEach(function (link) {
                    link.addEventListener('click', function () {
                        navLinks.forEach(function (nav) {
                            nav.classList.remove('active');
                        });
                        link.classList.add('active');
                    });
                });
            });
            document.addEventListener("DOMContentLoaded", function () {
                var navLinks = document.querySelectorAll('.nav-links');
                navLinks.forEach(function (link) {
                    link.addEventListener('click', function (event) {
                        navLinks.forEach(function (nav) {
                            nav.classList.remove('active_feature');
                        });
                        link.classList.add('active_feature');
                        event.preventDefault();
                    });
                });
            });
            document.addEventListener("DOMContentLoaded", function () {
                var navLinks = document.querySelectorAll('.nav-links');
                var currentActiveFeatureItem = document.getElementById('current-active-feature-item');
                var currentActiveFeature = document.getElementById('current-active-feature');
                //currentActiveFeatureItem.style.display = 'none';
                navLinks.forEach(function (link) {
                    link.addEventListener('click', function (event) {
                        navLinks.forEach(function (nav) {
                            nav.classList.remove('active_feature');
                        });
                        link.classList.add('active_feature');
                        var activeFeature = link.textContent || link.innerText;
                        currentActiveFeature.textContent = activeFeature;
                        currentActiveFeatureItem.style.display = 'inline';
                        event.preventDefault();
                    });
                });
            });
            window.addEventListener('scroll', function () {
                const header = document.querySelector('.header-section.sticky');
                if (header) {
                    const scrollTop = window.scrollY || document.documentElement.scrollTop;
                    if (scrollTop > 0) {
                        header.style.position = 'fixed';
                        header.style.top = '0';
                        header.style.left = '0';
                        header.style.right = '0';
                    } else {
                        header.style.position = 'sticky';
                    }
                }
            });
        </script>
    }
    @RenderSection("scripts", required: false)
    @Html.Partial("_ShoppingCartModal")
    @Html.Partial("_LoadingModal")
</body>
</html>
