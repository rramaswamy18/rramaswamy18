    <div style="float: left;">
        <img class="img-responsive" alt="@imageTitle" src="~/ClientSpecific/@(clientId)_@(clientName)/Documents/Images/Image_002.webp" title="@imageTitle" style="height: auto; max-height: 459px; max-width: 549px;" />
    </div>
    <div style="float: left; padding-left: 18px;">
        <span style="color: #000000; font-family: sans-serif; font-size: 18px; font-weight: 900;">
