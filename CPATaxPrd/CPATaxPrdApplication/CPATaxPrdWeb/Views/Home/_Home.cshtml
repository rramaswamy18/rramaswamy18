        </span>
    </div>
</div>
<script type="text/javascript">
    var carouselIndex = 0;
    displayCarousel();
    setInterval(displayCarousel, 4599);
    function displayCarousel() {
        for (var i = 0; i < 4; i++) {
            document.getElementById("divCarouselImageText" + i).style.display = "none";
        }
        document.getElementById("divCarouselImageText" + carouselIndex).style.display = "block";
        carouselIndex++;
        if (carouselIndex == 4) {
            carouselIndex = 0;
        }
    }
</script>
